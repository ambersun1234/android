<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_email"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginStart="160dp"
        android:layout_marginLeft="160dp"
        android:layout_marginTop="72dp"
        android:text="@string/btn_email"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_tel"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:text="@string/btn_tel"
        app:layout_constraintStart_toStartOf="@+id/btn_email"
        app:layout_constraintTop_toBottomOf="@+id/btn_email" />

    <Button
        android:id="@+id/btn_text"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:text="@string/btn_text"
        app:layout_constraintStart_toStartOf="@+id/btn_tel"
        app:layout_constraintTop_toBottomOf="@+id/btn_tel" />

    <Button
        android:id="@+id/btn_web"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:text="@string/btn_web"
        app:layout_constraintStart_toStartOf="@+id/btn_text"
        app:layout_constraintTop_toBottomOf="@+id/btn_text" />

    <Button
        android:id="@+id/btn_websearch"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:text="@string/btn_websearch"
        app:layout_constraintStart_toStartOf="@+id/btn_web"
        app:layout_constraintTop_toBottomOf="@+id/btn_web" />

    <Button
        android:id="@+id/btn_gps"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:text="@string/btn_gps"
        app:layout_constraintStart_toStartOf="@+id/btn_websearch"
        app:layout_constraintTop_toBottomOf="@+id/btn_websearch" />

</androidx.constraintlayout.widget.ConstraintLayout>